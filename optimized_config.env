# ðŸš€ JD-Fit Optimized Configuration for Final Run
# Copy this to .env and update with your actual values

# OpenAI Configuration (Required for Real Scoring)
JD_FIT_EMBEDDINGS__PROVIDER=openai
JD_FIT_EMBEDDINGS__MODEL=text-embedding-3-small
JD_FIT_EMBEDDINGS__BATCH_SIZE=256
JD_FIT_EMBEDDINGS__TIMEOUT_S=60

# OpenAI API Key (REQUIRED - Replace with your actual key)
OPENAI_API_KEY=sk-your-actual-openai-api-key-here

# Optional: LLM for Rationale Generation
JD_FIT_RATIONALE__PROVIDER=openai
JD_FIT_RATIONALE__MODEL=gpt-4

# Logging
JD_FIT_LOG_LEVEL=INFO

# Output Directory
JD_FIT_OUT_DIR=out

# Performance Tuning
JD_FIT_MAX_WORKERS=8
JD_FIT_BATCH_SIZE=32
